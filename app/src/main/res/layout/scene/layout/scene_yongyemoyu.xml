<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ScrollView
        android:id="@+id/scrollView"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingTop="60dp"
            android:paddingBottom="100dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_1"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="超阶恶魔 渊噬蛇皇"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_2"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="超阶恶魔 魔蟒·巴哈姆"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_3"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="超阶恶魔 暗影三头兽"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_4"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="圣阶恶魔 狮身魔牛"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_5"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="圣阶恶魔 紫翼魔使"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
