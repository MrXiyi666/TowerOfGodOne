<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ScrollView
        android:scrollbars="none"
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingTop="60dp"
            android:paddingBottom="100dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_1"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="初阶天使 晨光侍者·曦"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_2"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="初阶天使 星砂巡卫·洛"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_3"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="中阶天使 焰辉战使·赫萝"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_4"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="中阶天使 霜歌咏者·埃拉"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_5"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="高阶天使 雷罚统帅·卡西欧"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_6"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="高阶天使 圣辉祭司·艾蕾娜"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_7"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="超阶天使  破晓审判·赛勒"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_8"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="超阶天使 永夜守望·诺瓦"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_9"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="圣阶天使 苍穹御座·弥赛尔"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
            <ImageView
                android:padding="10dp"
                android:id="@+id/enemy_10"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <fun.android.towerofgodone.View.StrokeTextView
                android:text="圣阶天使 终焉福音·伊瑟拉"
                android:textSize="20sp"
                android:autoSizeTextType="uniform"
                android:textColor="@color/white"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <ImageView
            android:src="@drawable/cancel"
            android:layout_weight="0.4"
            android:id="@+id/button_cancel"
            android:layout_width="0dp"
            android:layout_height="60dp"/>
        <View
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</RelativeLayout>